{
  "name": "js-seed",
  "private": true,
  "version": "1.0.1",
  "description": "Seed your JS with ES6, webpack, babel, karma, jasmine, istanbul, config, git hooks",
  "main": "index.js",
  "scripts": {
    "build": "set NODE_ENV='production' && npm run :webpack:build",
    "clean": "npm run clean:build && npm run clean:logs && npm run clean:reports",
    "clean:build": "rm -f index.js && rm -f index.js.map",
    "clean:logs": "rm -f npm-debug.log",
    "clean:reports": "rm -Rf ./reports",
    "develop": "set NODE_ENV='development' && npm run :webpack:build:watch",
    "reset-deps": "rm -Rf node_modules && npm cache clean && npm install",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "echo 'No E2E tests, skipping...'",
    "test:unit": "set NODE_ENV='test' && npm run :karma",
    "test:unit:watch": "set NODE_ENV='test' && npm run :karma:watch",
    "test:unit:fast": "set FAST=1 && npm run test:unit",
    "test:unit:fast:watch": "set FAST=1 && npm run test:unit:watch",
    "precommit": "npm run test:unit:fast && npm run build",
    ":karma": "./node_modules/.bin/karma start config/karma.js",
    ":karma:watch": "npm run :karma -- --auto-watch --no-single-run",
    ":webpack:build": "./node_modules/.bin/webpack --config config/webpack.js --progress",
    ":webpack:build:watch": "npm run :webpack:build -- --watch"
  },
  "author": "Matthew Turnbull <turnbullm@gmail.com> (http://turnbullm.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "change-case": "^2.3.1",
    "config": "^1.26.2",
    "husky": "^0.11.9",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "jasmine-core": "^2.8.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-json-reporter": "^1.2.1",
    "karma-json-result-reporter": "^0.4.2",
    "karma-nyan-reporter": "^0.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "karma-polyfill": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.15",
    "webpack": "^1.15.0"
  }
}
